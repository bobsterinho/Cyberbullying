  <title>Cyberbullying Awareness Slides</title>
  <style>
    /* General Styles */
    body {
      margin: 0;
      font-family: Arial, sans-serif;
      background-color: #f4f4f9;
      overflow: hidden; /* Prevent scrolling */
    }

    .slide {
      position: absolute;
      top: 0;
      left: 100%; /* Start off-screen */
      width: 100%;
      height: 100vh;
      display: flex;
      justify-content: center;
      align-items: center;
      flex-direction: column;
      text-align: center;
      padding: 20px;
      transition: transform 0.5s ease-in-out;
      background-color: #ffffff;
      color: #333;
    }

    .slide.active {
      left: 0; /* Move to the visible area */
      transform: translateX(0); /* Transition into view */
    }

    .slide h1, .slide h2 {
      color: #007BFF;
    }

    .slide img {
      max-width: 60%;
      height: auto;
      margin: 20px 0;
      border-radius: 10px;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
    }

    .slide p, .slide ul {
      max-width: 600px;
      font-size: 1.2rem;
      line-height: 1.6;
    }

    ul {
      text-align: left;
    }

    .slide.hidden {
      display: none;
    }
  </style>
