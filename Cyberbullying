<body>
  <!-- Slide 1 -->
  <div class="slide active" id="slide-1">
    <h1>Cyberbullying Awareness</h1>
    <p>Welcome to my presentation on cyberbullying. Use the arrow keys to navigate.</p>
    <img src="data:image/jpeg;base64," alt="Cyberbullying Awareness">
  </div>

  <!-- Slide 2 -->
  <div class="slide hidden" id="slide-2">
    <h2>What is Cyberbullying?</h2>
    <p>Cyberbullying involves using technology to harm, harass, or intimidate others and it occurs on several social media platforms.</p>
    <img src="data:image/jpeg;base64," alt="What is Cyberbullying">
  </div>

  <!-- Slide 3 -->
  <div class="slide hidden" id="slide-3">
    <h2>How to Get Help</h2>
    <ul>
      <li>Talk to a trusted adult or friend.</li>
      <li>Contact a counselor or support organization.</li>
      <li>Block and report bullies on the platform.</li>
    </ul>
  </div>

  <!-- JavaScript for Slide Navigation -->
  <script>
    const slides = document.querySelectorAll('.slide');
    let currentSlide = 0;

    function showSlide(index) {
      // Ensure the index is within bounds
      if (index < 0) {
        index = slides.length - 1; // Wrap to the last slide
      } else if (index >= slides.length) {
        index = 0; // Wrap to the first slide
      }

      // Hide all slides and remove active class
      slides.forEach(slide => {
        slide.classList.add('hidden');
        slide.classList.remove('active');
      });

      // Show the current slide
      slides[index].classList.remove('hidden');
      slides[index].classList.add('active');
      currentSlide = index;
    }

    // Event listener for keyboard arrows
    document.addEventListener('keydown', (event) => {
      if (event.key === 'ArrowRight') {
        showSlide(currentSlide + 1); // Next slide
      } else if (event.key === 'ArrowLeft') {
        showSlide(currentSlide - 1); // Previous slide
      }
    });

    // Optionally, you can uncomment the next two lines to automatically transition every 5 seconds:
    // setInterval(() => {
    //   showSlide(currentSlide + 1);
    // }, 5000);
  </script>
</body>
</html>
